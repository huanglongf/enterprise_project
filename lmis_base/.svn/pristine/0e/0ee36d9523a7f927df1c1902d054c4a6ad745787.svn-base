/**
 * @author daikaihua
 * @date 2017年11月15日
 * @todo TODO
 */
@SpringBootApplication
@EnableEurekaClient
@ServletComponentScan
public class SpringBootSampleApplication {

    public static void main(String[] args) {
        SpringApplication.run(SpringBootSampleApplication.class, args);
    }
}
