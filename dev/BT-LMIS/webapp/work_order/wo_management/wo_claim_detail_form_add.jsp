<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- 索赔明细新增/编辑界面弹窗 -->
<div id= "wo_claim_detail_add" class= "modal fade" tabindex= "-1" role= "dialog" aria-labelledby= "formLabel" aria-hidden= "true" >
    <div class= "modal-dialog modal-lg" style= "width: 65%;" role= "document" >
        <div class= "modal-content" style= "border: 3px solid #394557;" >
            <div class= "modal-header" >
                <button type= "button" class= "close" data-dismiss= "modal" aria-label= "Close" ><span aria-hidden= "true" >×</span></button>
                <h4 id= "claim_detail_title" class= "modal-title" >新增索赔明细</h4>
            </div>
            <div class= "modal-body" style= "height: 450px;" >
           		<table class= "table table-striped" style= "table-layout: fixed;" >
			   		<thead>
				  		<tr class="table_head_line">
				  		    <td class="td_ch"><input type="checkbox" id="checkAll" onclick="inverseCkb('ckb')"/></td>
				  			<td class="td_cs" title="相关单据号" style= "width: 15%;" >相关单据号</td>
				  			<td class="td_cs" title="SKU编码" style= "width: 15%;" >SKU编码</td>
				  			<td class="td_cs" title="条形码" style= "width: 15%;" >条形码</td>
				  			<td class="td_cs" title="商品名称" style= "width: 20%;" >商品名称</td>
				  			<td class="td_cs" title="扩展名称" style= "width: 20%;" >扩展名称</td>
				  			<td class="td_cs" title="商品数量" style= "width: 10%;" >商品数量</td>
				  		</tr>  	
					</thead>
					<tbody id='tbody_id' align="center">		
					</tbody>
				</table>
            </div>
            <div class= "modal-footer" >
                <button type= "button" class= "btn btn-default"  data-dismiss= "modal" >
                    <i class= "icon-undo" aria-hidden= "true" ></i>返回
                </button>
                <button type= "button" class= "btn btn-primary" onclick= "addClaimDetail();" >
                    <i class= "icon-save" aria-hidden= "true" ></i>添加
                </button>
            </div>
        </div>
    </div>
</div>