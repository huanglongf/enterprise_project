<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!-- 预估批次弹窗 -->
<div id="batchNumber_Form" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="formLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg" style="width:40%;" role="document">
        <div class="modal-content" style="border:3px solid #394557;">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick= "closeWindows();"><span aria-hidden="true">×</span></button>
                <h4 class="modal-title">预估批次</h4>
            </div>
            <div class="modal-body container">
            	<div class="col-xs-4" style="text-align:right;">
                    <label class="no-padding-right blue" style="white-space: nowrap;">
                    	当前预估批次为&nbsp;:
                    </label>
                </div>
                <div class="col-xs-8">
                	<label style="cursor:pointer;" title="点击复制">
	             		<a id="retrunBatchNumber" class="a-copy red" data-clipboard-action="copy" data-clipboard-target="#retrunBatchNumber"></a>
	             	</label>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="closeWindows();;">
                    <i class="icon-undo" aria-hidden="true"></i>返回
                </button>
            </div>
        </div>
    </div>
</div>