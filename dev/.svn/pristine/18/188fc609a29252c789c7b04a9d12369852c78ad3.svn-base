package com.bt.lmis.balance.model;

import java.io.Serializable;
import java.util.Date;

/** 
 * @ClassName: RecoverProcess
 * @Description: TODO(这里用一句话描述这个类的作用)
 * @author Ian.Huang
 * @date 2017年7月14日 上午10:58:02 
 * 
 */
public class RecoverProcess implements Serializable {

	/** 
	 * @Fields serialVersionUID : TODO(用一句话描述这个变量表示什么) 
	 */
	private static final long serialVersionUID = 2549014724858345099L;
	private int id;
	private Date createTime;
	private String createBy;
	private int status;
	private int move;
	private int conId;
	private String recoverStartDate;
	private String recoverEndDate;
	public RecoverProcess() {}
	public RecoverProcess(String createBy, int move, int conId, String recoverStartDate, String recoverEndDate) {
		this.createBy = createBy;
		this.move = move;
		this.conId = conId;
		this.recoverStartDate = recoverStartDate;
		this.recoverEndDate = recoverEndDate;
		
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public Date getCreateTime() {
		return createTime;
	}
	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}
	public String getCreateBy() {
		return createBy;
	}
	public void setCreateBy(String createBy) {
		this.createBy = createBy;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public int getMove() {
		return move;
	}
	public void setMove(int move) {
		this.move = move;
	}
	public int getConId() {
		return conId;
	}
	public void setConId(int conId) {
		this.conId = conId;
	}
	public String getRecoverStartDate() {
		return recoverStartDate;
	}
	public void setRecoverStartDate(String recoverStartDate) {
		this.recoverStartDate = recoverStartDate;
	}
	public String getRecoverEndDate() {
		return recoverEndDate;
	}
	public void setRecoverEndDate(String recoverEndDate) {
		this.recoverEndDate = recoverEndDate;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	
}
