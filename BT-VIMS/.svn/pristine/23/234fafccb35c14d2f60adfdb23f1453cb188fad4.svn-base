package com.bt.vims.model;

import java.io.Serializable;

public class Visitors implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String visitor_name;//拜访者
	private String host;  //拜访人
	private String visitor_date;//拜访日期
	private String visitor_year;//拜访年
	private String content; //拜访事由
	
	
	public String getVisitor_name() {
		return visitor_name;
	}
	public void setVisitor_name(String visitor_name) {
		this.visitor_name = visitor_name;
	}
	public String getHost() {
		return host;
	}
	public void setHost(String host) {
		this.host = host;
	}
	public String getVisitor_date() {
		return visitor_date;
	}
	public void setVisitor_date(String visitor_date) {
		this.visitor_date = visitor_date;
	}
	public String getVisitor_year() {
		return visitor_year;
	}
	public void setVisitor_year(String visitor_year) {
		this.visitor_year = visitor_year;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	
	
	
	 
	
}
